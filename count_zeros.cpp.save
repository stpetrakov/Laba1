#include <iostream>
#include <cmath>
#include <ctime>
#include <stdlib.h>

typedef int T;
using namespace std;

int CountZeros (T x)
{
    size_t count = 0;

    while (x != 0) {
        count += !(x&1);
        x >>= 1;

    }

    return count;
}

uint32_t cnt5 ( uint32_t x ) {
x = ( x & 0x55555555u ) + ( ( x >> 1 ) & 0x55555555u ) ;
x = ( x & 0x33333333u ) + ( ( x >> 2 ) & 0x33333333u ) ;
x = ( x & 0x0f0f0f0fu ) + ( ( x >> 4 ) & 0x0f0f0f0fu ) ;
return x % 255u ;
}

int main()
{
    int x;
    cin >> x;
    cout << CountZeros (x) << ' ' << (x>>10);
}
